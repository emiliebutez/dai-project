		<servlet-class>controller.Tabtruevalidation</servlet-class>
		</servlet>
<servlet>
		<servlet-name>DepotJustificatifController</servlet-name>
		<display-name>DepotJustificatifController</display-name>
		<description></description>
		<servlet-class>controller.DepotJustificatifController</servlet-class>

		<init-param>
        	<param-name>chemin</param-name>
        	<param-value>/Tests_java/</param-value>
    	</init-param>
		<multipart-config>
     		<location>C:/Tests_java/</location>
     		<max-file-size>20848820</max-file-size>
     		<max-request-size>418018841</max-request-size>
     		<file-size-threshold>1048576</file-size-threshold>
		</multipart-config>

	</servlet>
	<servlet-mapping>
	<servlet-name>justificatifDocument</servlet-name>
	<url-pattern>/justificatifDocument</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ValidationController</servlet-name>
		<url-pattern>/ValidationController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>

		<servlet-name>Tabtruevalidation</servlet-name>
		<url-pattern>/Tabtruevalidation</url-pattern>
</servlet-mapping>
<servlet-mapping>
		<servlet-name>DepotJustificatifController</servlet-name>
		<url-pattern>/DepotJustificatifController</url-pattern>

	</servlet-mapping>
	
</web-app>
