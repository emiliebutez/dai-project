	<servlet>
		<servlet-name>ValidationController</servlet-name>
		<display-name>ValidationController</display-name>
		<description></description>
		<servlet-class>controller.ValidationController</servlet-class>
	</servlet>
	<servlet>

		<servlet-name>Tabtruevalidation</servlet-name>
		<display-name>Tabtruevalidation</display-name>
