<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>HelloWordController</servlet-name>
		<display-name>HelloWordController</display-name>
		<description></description>
		<servlet-class>controller.HelloWordController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ConnexionController</servlet-name>
